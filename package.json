{
  "name": "von-ui",
  "version": "0.1.2",
  "description": "vue移动端组件库",
  "keywords": [
    "components",
    "design",
    "vue",
    "vue-component",
    "ui",
    "Button"
  ],
  "homepage": "https://github.com/trayvon2017/von-ui",
  "repository": {
    "type": "git",
    "url": "git@github.com:trayvon2017/von-ui.git"
  },
  "main": "lib/vonui.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vonui --dest lib src/index.js && node build/gen-cssfile.js && gulp build --gulpfile theme/gulpfile.js",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name vonui --dest lib src/index.js",
    "build:theme": "node build/gen-cssfile.js && gulp build --gulpfile theme/gulpfile222.js",
    "build:theme2": "gulp build --gulpfile theme/gulpfile.js/index.js"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.1.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
